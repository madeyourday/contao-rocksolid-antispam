{
	"name":"madeyourday/contao-rocksolid-antispam",
	"description":"Protect your site effectively from spam without causing your visitors unnecessary difficulties.",
	"keywords":["contao","spam","form"],
	"type":"contao-module",
	"homepage":"https://rocksolidthemes.com/de/contao/plugins/honeypot-antispam",
	"license":"MIT",
	"authors":[
		{
			"name":"RockSolid Themes",
			"homepage":"https://rocksolidthemes.com/de/contao-themes",
			"role":"Developer"
		}
	],
	"support":{
		"forum":"http://help.rocksolidthemes.com/discussions/contao",
		"issues":"https://github.com/madeyourday/contao-rocksolid-antispam/issues",
		"source":"https://github.com/madeyourday/contao-rocksolid-antispam"
	},
	"require":{
		"php":">=5.5",
		"contao/core-bundle":"^4.3"
	},
	"conflict": {
		"contao/core": "*"
	},
	"autoload":{
		"psr-4": {
			"MadeYourDay\\RockSolidAntispam\\": "src/"
		}
	},
	"replace":{
		"contao-legacy/rocksolid-antispam":"self.version"
	}
}
